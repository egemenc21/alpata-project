{
  "compilerOptions": {
      
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "allowImportingTsExtensions": true,
      "module": "ES2015",
      "noEmit": true,
      
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": ".",
      "paths": {
          "*": ["node_modules/*", "src/types/*"]
      },
      
  },
  "include": ["./**/*"]
  }